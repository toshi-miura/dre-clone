<?xml version="1.0"?>
<dre-measurer-package version="1.0">
	<information>
		<key>jp.thisnor.dre.lls</key>
		<name>Loose Luminance Similarity</name>
		<version>1.2</version>
		<author>jp.thisnor</author>
		<caption>!CAPTION!</caption>
		<description>!DESCRIPTION!</description>
	</information>
	<locale>
		<lang>jp.thisnor.dre.lls.lang</lang>
	</locale>
	<class>
		<measurer>jp.thisnor.dre.lls.LLSMeasurer</measurer>
	</class>
	<options>
		<option>
			<key>distType</key>
			<name>!OPTION_DISTTYPE!</name>
			<select>
				<value>Euclidean</value>
				<value>Manhattan</value>
				<value>Chebyshev</value>
			</select>
			<default>Euclidean</default>
		</option>
		<option>
			<key>threshold</key>
			<name>!OPTION_THRESHOLD!</name>
			<default>100</default>
		</option>
		<option>
			<key>useCache</key>
			<name>!OPTION_USECACHE!</name>
			<select>
				<value>!OPTION_USECACHE_TRUE!</value>
				<value>!OPTION_USECACHE_FALSE!</value>
			</select>
			<default>!OPTION_USECACHE_TRUE!</default>
		</option>
	</options>
</dre-measurer-package>